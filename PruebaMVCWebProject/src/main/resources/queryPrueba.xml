<?xml version="1.0" encoding="UTF-8"?>
<QUERY>
<QUERY_NAME> QUERY PRUEBA</QUERY_NAME>
	<SQL_QUERY><![CDATA[select * from unidad where id_unidad in(:ID) or 
	(cod_unidad=:UNIDAD and nombre=:NOMBRE )]]></SQL_QUERY>
	<PARAMETER NAME="ID" TYPE="INTEGER" />
	<PARAMETER NAME="UNIDAD" TYPE="VARCHAR" />
	<PARAMETER NAME="NOMBRE" TYPE="VARCHAR" />
		
	
</QUERY>